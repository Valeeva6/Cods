#include <iostream>

using namespace std;

int main () 
{
    int a, b, sum = 0;
    
    cout << "Enter a (left border): ";
    cin >> a;
    
    cout << "Enter b (right border): ";
    cin >> b;
    
    for (int i=a; i<b; i++) {
        for (int j=a; j<i; j++) {
            
            if (i % j == 0) { 
                break;
            }
            
            else if (i == j+1) {
                sum = sum + i;
            }
        }   
    }
    
    cout << "Result: " << sum << endl;
    return 0;
}
