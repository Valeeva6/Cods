#include <iostream>
using namespace std;
int main()
{
    int a, a1, a2, a3, a4, result;
    
    cout << "Enter a: ";
    cin >> a;
    
    a1 = a / 1000;
    a2 = a / 100 % 10;
    a3 = a % 100 / 10;
    a4 = a % 10;
    
    result = a4 * 1000 + a3 * 100 + a2 * 10 + a1;
    
    cout << result << endl;
    
    return 0;
}
