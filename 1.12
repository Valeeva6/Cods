#include <iostream>
#include <string>

using namespace std;

int main() {
    setlocale(LC_ALL, "Russian");
    
    cout << "Введите строку: ";
    string s;
    getline(cin, s);
    
    for (int i = 0; i < s.length(); i++) {
        if (s[i] == ' ') {
            cout << endl;
        }
        else {
            cout << s[i];
        }
    }
    
    return 0;
}
